{
  "name": "wowza-webrtc-js",
  "version": "0.0.1",
  "description": "Wowza WebRTC JS Library",
  "main": "dist/webrtc.min.js",
  "repository": "https://github.com/hirokazumiyaji/wowza-webrtc-js.git",
  "author": "Hirokazu Miyaji",
  "license": "MIT",
  "scripts": {
    "build:development": "webpack --cache false --mode development --output-filename [name].js --config webpack.config.js",
    "build:production": "webpack --cache false --mode production --output-filename [name].min.js --config webpack.config.js",
    "format": "prettier --write --no-semi --single-quote src/*.js",
    "lint": "eslint ./src",
    "dev": "http-server ./example"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/polyfill": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-flow": "^7.0.0-beta.44",
    "@babel/register": "^7.0.0-beta.44",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.2",
    "babel-polyfill": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.69.0",
    "http-server": "^0.11.1",
    "prettier": "^1.12.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
